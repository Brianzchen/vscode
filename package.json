{
  "name": "file-icons",
  "displayName": "file-icons",
  "description": "File-specific icons in VSCode for improved visual grepping.",
  "version": "0.0.1",
  "publisher": "file-icons",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "file-icons",
        "label": "File Icons",
        "path": "./icons/file-icons-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "import": "node ./scripts/import.js"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "cson": "^4.0.0",
    "genex": "^0.1.3",
    "parameterize": "0.0.7",
    "ret": "^0.1.13"
  },
  "author": "Daniel Brooker <dan@nocturnalcode.com>",
  "license": "MIT"
}

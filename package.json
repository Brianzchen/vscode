{
  "name": "file-icons",
  "displayName": "file-icons",
  "description": "File-specific icons in VSCode for improved visual grepping.",
  "version": "0.0.1",
  "publisher": "file-icons",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "file-icons",
        "label": "File Icons",
        "path": "./icons/file-icons-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "import": "node ./scripts/import.js"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "cson": "^4.0.0"
  }
}
